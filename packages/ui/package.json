{
  "name": "@nemesis/ui",
  "version": "0.0.0",
  "description": "A completely generic and boring UI components library",
  "private": true,
  "type": "module",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "svelte": "./src/index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",

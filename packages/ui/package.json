    "clean": "rimraf node_modules .turbo"
  },
  "devDependencies": {
    "@nemesis/eslint-config": "workspace:*",
    "@nemesis/tsconfig": "workspace:*",
    "svelte": "4.2.2",
    "tailwind-config": "workspace:*"
  }
}